<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="170.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.searchController">
   <children>
      <VBox alignment="CENTER" prefHeight="170.0" prefWidth="200.0">
         <children>
            <HBox prefHeight="50.0" prefWidth="200.0" spacing="12">
               <children>
                  <Label text="From" />
                  <DatePicker fx:id="lend_from" editable="false" showWeekNumbers="true" prefWidth="120" />
               </children>
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
               <padding>
                  <Insets left="15.0" />
               </padding>
            </HBox>
            <HBox prefHeight="50.0" prefWidth="200.0" spacing="30">
               <children>
                  <Label text="To" />
                  <DatePicker fx:id="lend_to" editable="false" showWeekNumbers="true" prefWidth="120" />
               </children>
               <padding>
                  <Insets left="15.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
